{
  "presets": [
    ["@babel/preset-env", {
      "modules": "commonjs"
    }],
    "@babel/preset-typescript"
  ],
  "plugins": [
    "@babel/plugin-syntax-typescript",
    "@babel/plugin-transform-typescript",
    ["@babel/plugin-transform-modules-commonjs", {}]
  ],
  "ignore": [
    "**/coverage"
  ],
  "targets": {
    "node": 12
  },
  "sourceMaps": true
}
