{
  "name": "atlan",
  "version": "0.0.1",
  "description": "Atlan is a CLI to manage local dockerized apps",
  "main": "dist/src/index.js",
  "files": [
    "dist"
  ],
  "bin": {
    "atlan": "dist/src/cli.js",
    "atl": "dist/src/cli.js"
  },
  "scripts": {
    "exec": "npx tsc && node dist/src/bin.js",
    "lint": "eslint --ext .ts ./src",
    "lint:fix": "npm run lint -- --fix",
    "build": "tsc -p .",
    "build:watch": "npx tsc -w",
    "local": "sudo npm i -g && atlan",
    "refresh": "rm -rf ./node_modules ./package-lock.json && npm install",
    "run:dev": "ts-node ./src/index.ts",
    "test": "jest"
  },
  "author": "Myastr0",
  "repository": {
    "type": "git",
    "url": "https://github.com/Myastr0/atlan.git"
  },
  "license": "MIT",
  "devDependencies": {
    "@commitlint/cli": "17.0.3",
    "@commitlint/config-conventional": "17.0.3",
    "@types/jest": "28.1.3",
    "@types/mock-fs": "4.13.1",
    "@typescript-eslint/eslint-plugin": "5.31.0",
    "@typescript-eslint/parser": "5.31.0",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "8.20.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-import-resolver-typescript": "3.3.0",
    "eslint-plugin-import": "2.26.0",
    "husky": "4.3.8",
    "jest": "28.1.3",
    "mock-fs": "5.1.4",
    "nodemon": "2.0.19",
    "prettier": "2.7.1",
    "ts-jest": "28.0.7",
    "ts-node": "10.9.1",
    "typescript": "4.7.4"
  },
  "dependencies": {
    "@types/clear": "0.1.2",
    "@types/figlet": "1.5.4",
    "@types/fs-extra": "9.0.13",
    "@types/inquirer": "8.2.1",
    "@types/node": "18.6.2",
    "@types/shelljs": "0.8.11",
    "chalk": "4.1.2",
    "clear": "0.1.0",
    "commander": "9.4.0",
    "figlet": "1.5.2",
    "fs-extra": "10.1.0",
    "inquirer": "7.0.3",
    "module-alias": "2.2.2",
    "open-editor": "3.0.0",
    "ora": "5.4.1",
    "shelljs": "0.8.5",
    "winston": "3.8.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "_moduleAliases": {
    "@config": "./dist/src/config/index.js",
    "@state": "./dist/src/state/index.js"
  }
}
